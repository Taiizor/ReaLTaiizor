    <None Remove="Bitmaps\TabControl.bmp" />
    <None Remove="Bitmaps\TextBox.bmp" />
    <None Remove="Bitmaps\ToolTip.bmp" />
    <None Remove="Resources\Material\Roboto-Black.ttf" />
